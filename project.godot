config_version=5

[application]

config/name="War Eagles Digital"
run/main_scene="uid://c2kovfcv787jl"
config/features=PackedStringArray("4.4", "C#", "GL Compatibility")
run/max_fps=60
run/delta_smoothing=false
config/icon="uid://c3a8wgfq43mga"

[autoload]

UnitDatabase="*res://Scripts/UnitDatabase.cs"
AudioManager="*res://Scripts/AudioManager.cs"

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="War Eagles Digital"

[editor_plugins]

enabled=PackedStringArray("res://addons/terrain_3d/plugin.cfg")

[physics]

2d/run_on_separate_thread=true
3d/run_on_separate_thread=true
common/max_physics_steps_per_frame=2
2d/physics_engine="Dummy"
jolt_physics_3d/simulation/velocity_steps=2
jolt_physics_3d/simulation/use_enhanced_internal_edge_removal=false
jolt_physics_3d/motion_queries/use_enhanced_internal_edge_removal=false
jolt_physics_3d/limits/world_boundary_shape_size=600.0
common/physics_interpolation=true

[rendering]

